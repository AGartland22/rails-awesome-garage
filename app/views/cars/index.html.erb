<h1>Cars</h1>

<% @cars.each do |car| %>
  <h2><%= car.brand %></h2>
  <h3><%= car.model %></h3>
  <p><%= car.created_at.strftime("%a %b %e at %l:%M %p") %></p>
  <p><%= car.owner.nickname%></p>

  <div class="cards">
    <%= link_to car_path(car) do %>
      <h2> <%= car.model %> </h2>
    <% end %>
  </div>
  <hr>
<% end %>
